{
    "name": "ytokarchukova/oauth-5-laravel",
    "type": "library",
    "description": "OAuth Service Provider for Laravel 5",
    "keywords": ["OAuth", "Lusitanian", "laravel", "php"],
    "homepage": "https://github.com/ytokarchukova/oauth-5-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Artdarek",
            "email": "artdarek@gmail.com",
            "role": "Developer"
        },
        {
            "name": "oriceon",
            "email": "oriceon@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Yulia Tokarchukova",
            "email": "ytokarchukova@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ytokarchukova/oauth": "dev-master"
    },
    "require-dev": {
        "illuminate/support": "~5"
    },
    "require-all": true,
    "extra": {
        "laravel": {
            "providers": [
                "Artdarek\\OAuth\\OAuthServiceProvider"
            ],
            "aliases": {
                "OAuth" : "Artdarek\\OAuth\\Facade\\OAuth"
            }
        }
    },
    "autoload": {    
        "psr-0": {
            "Artdarek\\OAuth": "src/"
        }
    },    
    "minimum-stability": "dev"
}
